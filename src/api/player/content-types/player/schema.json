{
  "kind": "collectionType",
  "collectionName": "players",
  "info": {
    "singularName": "player",
    "pluralName": "players",
    "displayName": "Player"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "player"
    },
    "elo": {
      "type": "integer",
      "required": true,
      "default": 600
    },
    "matchesHistory": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::match.match"
    },
    "registrations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::registration.registration",
      "inversedBy": "players"
    },
    "league": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::league.league",
      "inversedBy": "players"
    },
    "firstname": {
      "type": "string",
      "required": true
    },
    "lastname": {
      "type": "string",
      "required": true
    }
  }
}
