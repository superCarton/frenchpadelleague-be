{
  "kind": "collectionType",
  "collectionName": "matches",
  "info": {
    "singularName": "match",
    "pluralName": "matches",
    "displayName": "Match"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "teamA": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::team.team"
    },
    "teamB": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::team.team"
    },
    "date": {
      "type": "datetime",
      "required": true
    },
    "tournament": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::tournament.tournament",
      "inversedBy": "matches"
    },
    "scoreTeamA": {
      "type": "json"
    },
    "scoreTeamB": {
      "type": "json"
    },
    "winner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::team.team"
    },
    "matchStatus": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "scheduled",
        "started",
        "finished"
      ]
    },
    "scheduledDate": {
      "type": "datetime"
    }
  }
}
