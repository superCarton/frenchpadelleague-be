{
  "kind": "collectionType",
  "collectionName": "matches",
  "info": {
    "singularName": "match",
    "pluralName": "matches",
    "displayName": "Match"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "teamA": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::team.team"
    },
    "teamB": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::team.team"
    },
    "date": {
      "type": "datetime",
      "required": false
    },
    "winner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::team.team"
    },
    "matchStatus": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "scheduled",
        "started",
        "finished"
      ]
    },
    "scheduledDate": {
      "type": "datetime"
    },
    "tournament_group": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::tournament-group.tournament-group",
      "inversedBy": "matches"
    },
    "tournament_phase": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::tournament-phase.tournament-phase",
      "inversedBy": "matches"
    },
    "game_format": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::game-format.game-format"
    }
  }
}
