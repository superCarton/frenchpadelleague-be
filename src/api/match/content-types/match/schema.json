{
  "kind": "collectionType",
  "collectionName": "matches",
  "info": {
    "singularName": "match",
    "pluralName": "matches",
    "displayName": "Match"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "team_a": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::team.team"
    },
    "team_b": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::team.team"
    },
    "date": {
      "type": "datetime",
      "required": false
    },
    "matchStatus": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "scheduled",
        "started",
        "finished"
      ]
    },
    "scheduledDate": {
      "type": "datetime"
    },
    "tournament_group": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::tournament-group.tournament-group",
      "inversedBy": "matches"
    },
    "game_format": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::game-format.game-format"
    },
    "name": {
      "type": "string",
      "required": true
    },
    "tournament": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::tournament.tournament",
      "inversedBy": "matches"
    },
    "score": {
      "type": "component",
      "component": "shared.match-set",
      "repeatable": true
    },
    "winner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::team.team"
    },
    "round": {
      "type": "enumeration",
      "enum": [
        "r32",
        "r16",
        "quarter",
        "semi",
        "final"
      ]
    }
  }
}
